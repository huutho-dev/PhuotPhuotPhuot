<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@drawable/background_see"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/act_home_imv_north"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/background_main_bac" />

    <ImageView
        android:id="@+id/act_home_imv_central"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/background_main_trung" />

    <ImageView
        android:id="@+id/act_home_imv_south"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/background_main_nam"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>